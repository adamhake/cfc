{
  "name": "@chimborazo/sanity-config",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./schemas": {
      "import": "./src/schemas/index.ts",
      "types": "./src/schemas/index.ts"
    },
    "./queries": {
      "import": "./src/queries/index.ts",
      "types": "./src/queries/index.ts"
    },
    "./client": {
      "import": "./src/client.ts",
      "types": "./src/client.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "format": "prettier --write src"
  },
  "dependencies": {
    "@sanity/client": "^6.24.2",
    "@sanity/image-url": "^1.1.0",
    "groq": "^4.15.0",
    "sanity": "^3.68.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  }
}
